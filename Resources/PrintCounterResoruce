import java.io.BufferedReader; //These are needed for reading the python file
import java.io.FileReader;
import java.io.IOException;

//meant to read a file called PythonTest.py. Create one for testing if you want.

public class PrintFunctionCounter {

    public static void main(String[] args) {
        // Replace filePath with the path to your file
        String filePath = "PythonTest.py";
        String functionName = "print(";
        int count = 0;

        try (BufferedReader br = new BufferedReader(new FileReader(filePath))) {
            String line;
            while ((line = br.readLine()) != null) {
                // Check if the line contains the function name
                if (line.contains(functionName)) {
                    // If yes, increment the count
                    count++;
                }
            }
        } catch (IOException e) {
            e.printStackTrace();
        }

        System.out.println("Occurrences of function '" + functionName + "': " + count);
    }
}